<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>JonasSchubert.Snippets.Math</AssemblyName>
    <RootNamespace>JonasSchubert.Snippets.Math</RootNamespace>
    <Version>0.1.1</Version>
    <Authors>Jonas Schubert</Authors>
    <Company>GuepardoApps</Company>
    <Description>A curated collection of useful C# snippets about math that you can understand in 30 seconds or less.</Description>
    <Copyright>JonasSchubert</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/JonasSchubert/30-seconds-of-c-sharp/projects/1</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JonasSchubert/30-seconds-of-c-sharp/</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
